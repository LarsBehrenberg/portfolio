{
  "name": "larsbehrenberg.com",
  "description": "Freelance Website Developer website built with Gatsby",
  "private": true,
  "license": "MIT",
  "version": "1.0.0",
  "author": "Lars Behrenberg <info@larsbehrenberg.com>",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop -o",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "format": "prettier \"**/*.md \" --write",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:ci": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run:ci"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@emotion/styled-base": "^10.0.31",
    "@reach/skip-nav": "^0.11.2",
    "emotion": "^10.0.27",
    "emotion-server": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.24.55",
    "gatsby-image": "^2.4.17",
    "gatsby-plugin-catch-links": "^2.3.11",
    "gatsby-plugin-emotion": "^4.3.10",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-lodash": "^3.3.10",
    "gatsby-plugin-manifest": "^2.4.28",
    "gatsby-plugin-netlify": "^2.3.14",
    "gatsby-plugin-offline": "^3.2.27",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.33",
    "gatsby-plugin-sitemap": "^2.4.12",
    "gatsby-plugin-typography": "^2.5.10",
    "gatsby-source-prismic": "^2.2.0",
    "gatsby-transformer-sharp": "^2.5.14",
    "lodash": "^4.17.20",
    "mailtoui": "^1.0.3",
    "prismic-reactjs": "^0.3.2",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-burger-menu": "^2.7.1",
    "react-dom": "^16.13.1",
    "react-flip-toolkit": "^7.0.12",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-particles-js": "^2.7.1",
    "react-transition-group": "^4.4.1",
    "react-typography": "^0.16.19",
    "typeface-nunito": "^1.1.3",
    "typeface-source-sans-pro": "^1.1.5",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.1.0",
    "cypress-testing-library": "^4.0.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gatsby-cypress": "^0.4.10",
    "prettier": "^2.1.1",
    "start-server-and-test": "^1.11.3"
  },
  "keywords": [
    "gatsby",
    "starter",
    "prismic",
    "typography",
    "minimal",
    "gatsby-starter"
  ]
}
